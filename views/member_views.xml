                            <field name="interfederation_id"/>
                            <field name="federation_id"/>
                            <field name="district_id"/>
                            <field name="section_id"/>
                            <field name="cellule_id"/>
                            <field name="souscellule_id"/>
                        </group>
                    </group>
                </page>
            </notebook>
        </field>
    </record>

