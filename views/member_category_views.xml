<odoo>
    <record id="member_category_tree" model="ir.ui.view">
        <field name="name">membership.member.category.list</field>
        <field name="model">membership.member.category</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
                <field name="description"/>
            </list>
        </field>
    </record>




    <record id="member_category_form" model="ir.ui.view">
        <field name="name">membership.member.category.form</field>
        <field name="model">membership.member.category</field>
        <field name="arch" type="xml">
            <form string="Categories">
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="description"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

</odoo>
